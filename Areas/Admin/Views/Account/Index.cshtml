@model IEnumerable<AppleStore.Models.Account>

@{
    ViewData["Title"] = "Quản lý tài khoản";
}

<h1>Danh sách tài khoản</h1>


<table class="table table-bordered mt-3">
    <thead>
        <tr>
            <th>Tên đăng nhập</th>
            <th>Quyền hạn</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var account in Model)
        {
            <tr>
                <td>@account.Username</td>
                <td> @account.Role</td>
            </tr>
        }
    </tbody>

</table>
